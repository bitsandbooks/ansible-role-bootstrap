# jail.local
# Managed by Ansible - DO NOT EDIT BY HAND
# (unless you know what you're doing)

[ssh]
enabled  = true
port     = ssh
filter   = sshd
logpath  = /var/log/auth.log
maxretry = 6

[nginx-http-auth]
enabled = false
filter  = nginx-http-auth
port    = http,https
logpath = /var/log/nginx/error.log
